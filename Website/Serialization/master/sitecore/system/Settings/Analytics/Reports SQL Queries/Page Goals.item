template: {A90F48C4-4D4F-41D7-8497-070A9A50304E}
templatekey: ReportQuery

----field----
field: {A61CC636-3272-4FB6-BCF9-762940047A78}
name: SqlServer
key: sqlserver
content-length: 324

SELECT TOP 5
    [PageEventName],
    COUNT(*) [VisitsCount],
    SUM([Value]) [Value]
FROM
    [VisitEventsByPage]
WHERE
    [IsGoal] = 1
    AND [ItemId] = @Id
    AND [Date] BETWEEN CONVERT(nvarchar(8), @StartDate, 112) AND CONVERT(nvarchar(8), @EndDate, 112)
GROUP BY [PageEventName]
ORDER BY [Value] DESC  

----field----
field: {E6A8E2B9-2111-40A2-B4AC-88A607E68307}
name: Oracle
key: oracle
content-length: 347

SELECT * FROM (SELECT 
    "PAGEEVENTNAME",
    COUNT(*) "VISITSCOUNT",
    SUM("VALUE") "VALUE"
FROM
    "VISITEVENTSBYPAGE"
WHERE
    "ISGOAL" = 1
    AND "ITEMID" = :Id
    AND "DATE" BETWEEN TO_CHAR ( :StartDate, 'YYYYMMDD') AND TO_CHAR ( :EndDate, 'YYYYMMDD')
GROUP BY "PAGEEVENTNAME"
ORDER BY "VALUE" DESC  
) WHERE ROWNUM <= 5

