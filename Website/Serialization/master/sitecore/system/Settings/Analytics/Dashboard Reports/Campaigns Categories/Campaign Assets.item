CampaignAssets
----field----
field: {46FB3CB4-4740-4680-BB76-63A15EF25907}
name: SqlServer
key: sqlserver
content-length: 864

select 
	[Month] as [Date],
	COUNT(*) as [Visits],
	SUM([Value]) as [Value],
	[DataKey] as [FacetName]
from
(select V.[Date]
      ,V.[Month]
      ,V.[TrafficType]
      ,V.[CampaignId]
      ,V.[VisitId]
      ,V.[PageEventDefinitionId]
      ,V.[PageEventName]
      ,V.[DataKey]
      ,V.[Value]
      ,V.[IsGoal]
      ,V.[NumberOfEvents]
      ,V.[MultiSite]
      ,V.[Language]
      ,C.[CampaignName]
      ,C.[Category1Id]
      ,C.[Category2Id]
      ,C.[Category3Id]
  from [VisitEventsByDataKey] V, [Campaigns] C 
  where
    V.[CampaignId] = C.[CampaignId]
    and [IsGoal] = 0
	  and [DataKey] > ' '
	  and V.[CampaignId] is not null
  	and [PageEventDefinitionId] = 'fa72e131-3cfd-481c-8e15-04496e9586dc'
  ) DV
where 
  1 = 1
	{Filters}
group by
	[Month], [DataKey]
having
  SUM([Value]) > 0
order by
	[Month]
----field----
field: {A9C8D7C7-A170-4305-A40F-DF6CF543C007}
name: Oracle
key: oracle
content-length: 864

select 
	"MONTH" as "DATE",
	COUNT(*) as "VISITS",
	SUM("VALUE") as "VALUE",
	"DATAKEY" as "FACETNAME"
from
(select V."DATE"
      ,V."MONTH"
      ,V."TRAFFICTYPE"
      ,V."CAMPAIGNID"
      ,V."VISITID"
      ,V."PAGEEVENTDEFINITIONID"
      ,V."PAGEEVENTNAME"
      ,V."DATAKEY"
      ,V."VALUE"
      ,V."ISGOAL"
      ,V."NUMBEROFEVENTS"
      ,V."MULTISITE"
      ,V."LANGUAGE"
      ,C."CAMPAIGNNAME"
      ,C."CATEGORY1ID"
      ,C."CATEGORY2ID"
      ,C."CATEGORY3ID"
  from "VISITEVENTSBYDATAKEY" V, "CAMPAIGNS" C 
  where
    V."CAMPAIGNID" = C."CAMPAIGNID"
    and "ISGOAL" = 0
	  and "DATAKEY" > ' '
	  and V."CAMPAIGNID" is not null
  	and "PAGEEVENTDEFINITIONID" = 'fa72e131-3cfd-481c-8e15-04496e9586dc'
  ) DV
where 
  1 = 1
	{Filters}
group by
	"MONTH", "DATAKEY"
having
  SUM("VALUE") > 0
order by
	"MONTH"
