CampaignListByDay
----field----
field: {46FB3CB4-4740-4680-BB76-63A15EF25907}
name: SqlServer
key: sqlserver
content-length: 255

select
	[Date] as [Date],
	SUM([Visits]) [Visits],
	SUM([Value]) [Value],
	[CampaignId] [FacetId],
	[CampaignName] [FacetName]
from 
	[CampaignsOverview]
where
	1 = 1
	{Filters}
group by
	[Date], [CampaignId], [CampaignName]
order by
	[Date]
----field----
field: {A9C8D7C7-A170-4305-A40F-DF6CF543C007}
name: Oracle
key: oracle
content-length: 255

select
	"DATE" as "DATE",
	SUM("VISITS") "VISITS",
	SUM("VALUE") "VALUE",
	"CAMPAIGNID" "FACETID",
	"CAMPAIGNNAME" "FACETNAME"
from 
	"CAMPAIGNSOVERVIEW"
where
	1 = 1
	{Filters}
group by
	"DATE", "CAMPAIGNID", "CAMPAIGNNAME"
order by
	"DATE"
